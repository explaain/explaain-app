<h2>
  {{name}}
</h2>
<!-- <div class="card-image">{{image}}</div> -->
<div class="body-content">
  {{{description}}}
  <div class="matches">
    {{#matches}}
      <div class="match">
        <h3 class="question">
          {{{question}}}
        </h3>
        <div class="userOpinion">
          <b>Your choice:</b> {{{userOpinion}}}
        </div>
        <div class="partyOpinion">
          <b>Their choice:</b> {{{partyOpinion}}}
        </div>
      </div>
    {{/matches}}
  </div>
</div>
